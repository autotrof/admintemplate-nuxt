{
  "nuxt.isNuxtApp": true,
  "css.lint.unknownAtRules": "ignore",
  "*.css": "tailwindcss",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.files.exclude":[
    "**/.git/**", "**/node_modules/**", "**/.hg/**", "**/public/**"
  ],
  "tailwindCSS.emmetCompletions": true
}